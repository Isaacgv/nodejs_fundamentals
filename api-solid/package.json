  "devDependencies": {
    "@types/node": "22.8.1",
    "tsup": "8.3.5",
    "tsx": "4.19.1",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "fastify": "5.0.0"
  }
}
